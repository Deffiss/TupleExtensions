<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <PackageId>TupleExtensions.VictorGavrish</PackageId>
    <PackageVersion>1.0.0</PackageVersion>
    <Authors>Victor Gavrish</Authors>
    <Description>A collection of convenience extensions that leverages C# 7 tuples</Description>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageReleaseNotes>First release</PackageReleaseNotes>
    <Copyright>Copyright 2017 (c) Victor Gavrish. All rights are reserved.</Copyright>
    <PackageTags>linq tuple tuples</PackageTags>
    <AssemblyName>TupleExtensions</AssemblyName>
    <RootNamespace>TupleExtensions</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard1.4\TupleExtensions.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="System.ValueTuple" Version="4.3.0" />
  </ItemGroup>

</Project>